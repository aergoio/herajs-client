{
  "name": "@herajs/client",
  "version": "0.2.2",
  "description": "Javascript client framework for aergo",
  "main": "dist/herajs.common.js",
  "module": "dist/herajs.esm.js",
  "browser": "dist/herajs.min.js",
  "types": "dist/platforms/node/index.d.ts",
  "scripts": {
    "test": "npm run lint && mocha -r babel-register-ts",
    "dev": "rollup -w -c scripts/config.js --environment TARGET:node-esm",
    "lint": "eslint --fix src scripts test",
    "grpc": "./scripts/gen_proto.sh",
    "release": "bash scripts/release.sh",
    "build": "babel-node scripts/build.js",
    "docs": "cd docs && make html",
    "type-check": "tsc --noEmit src/index",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aergoio/herajs.git"
  },
  "author": "aergo team and contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aergoio/herajs/issues"
  },
  "homepage": "https://github.com/aergoio/herajs#readme",
  "aergo": {
    "version": "v0.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@herajs/crypto": "0.0.1",
    "@types/google-protobuf": "^3.2.7",
    "@types/rollup-plugin-json": "^3.0.0",
    "async-sema": "^2.1.4",
    "babel-register-ts": "^7.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "crypto-js": "^3.1.9-1",
    "eslint": "^5.2.0",
    "grpc-tools": "^1.6.6",
    "mocha": "^5.2.0",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.65.2",
    "rollup-plugin-analyzer": "^2.1.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-terser": "^2.0.2",
    "rollup-plugin-typescript2": "^0.18.0",
    "ts-protoc-gen": "^0.7.6",
    "typescript": "^3.1.3",
    "uglify-js": "^3.4.5"
  },
  "dependencies": {
    "bs58": "^4.0.1",
    "bs58check": "^2.1.2",
    "google-protobuf": "^3.6.0",
    "grpc": "^1.13.1",
    "grpc-web-client": "^0.6.2",
    "jsbi": "^2.0.5"
  }
}
